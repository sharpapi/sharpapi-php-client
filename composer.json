{
  "name": "sharpapi/sharpapi-php-client",
  "description": "SharpAPI - AI-Powered Swiss Army Knife API. Assisting coders with the most repetitive content analysis and content generation processing needs of any app or platform.",
  "keywords": [
    "SharpAPI",
    "API",
    "AI API",
    "automation",
    "API Integration",
    "Content Processing",
    "App Enhancement"
  ],
  "homepage": "https://github.com/sharpapi/sharpapi-php-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Dawid Makowski",
      "email": "contact@sharpapi.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.8",
    "kongulov/interact-with-enum": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "SharpAPI\\SharpApiService\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SharpAPITest\\SharpApiService\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": "",
    "build": [
    ],
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@composer run build"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  }
}
